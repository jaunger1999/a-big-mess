thingdelta
{
  name DoomBlueCard

  pickupeffect
  { 
    effects BlueCard
    message $GOTBLUECARD
    sound   "getkey"
    flags   ALWAYSPICKUP|LEAVEINMULTI
  }

  states
  @"
  Spawn:
    BKEY ABC 4 bright
    BKEY DEF 4
    BKEY GHI 4 bright
    BKEY JKL 4
    loop
  "@
}

